@keyframes slide {
  from {
    /*right: .3rem;*/
    opacity: 0.2; }
  to {
    /*right: .2rem;*/
    opacity: 0.7; } 
}

@-webkit-keyframes slide {
  from {
    /*right: .3rem;*/
    opacity: 0.2; }
  to {
    /*right: .2rem;*/
    opacity: 0.7; } 
}

.verticalPage .swiper-slide .bg_logo02{
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
}
.verticalPage .swiper-slide .dir{
    position: absolute;
    width: 100%;
    color: #222;
    font-size: .25rem;
    left: 0;
    top: 25%;
    text-align: center;
}
.verticalPage .swiper-slide .dir02{
    position: absolute;
    width: 100%;
    color: #222;
    font-size: .25rem;
    left: 0;
    top: 4%;
    text-align: center;
}
.verticalPage .swiper-slide .con{
    position: absolute;
    width: 100%;
    left: 0;
    top: 34%;
}
.verticalPage .swiper-slide .con p{
    color: #222;
    font-size: .16rem;
    text-align: center;
    line-height: .36rem;
}
.verticalPage .swiper-slide .detail{
    position: absolute;
    left: 0;
    top: 4%;
    width: 100%;
    /*overflow-y: scroll;
    height: 70%;*/
}
.verticalPage .swiper-slide .detail_top{
    position: absolute;
    left: 0;
    top: 10%;
    width: 100%;
    overflow-y: scroll;
    height: 82%;
}
.baogaoLookMore {
    position: fixed;
    width: 100%;
    text-align: center;
    color: #05a1ff;
    font-size: .11rem;
    bottom: .30rem;
    z-index: 9999
}
.verticalPage .swiper-slide .detail02{
    position: absolute;
    left: 0;
    top: 3%;
    width: 100%;
}
.verticalPage .swiper-slide .detail h2{
    color: #05a1ff;
    font-size: .16rem;
    text-align: center;
    line-height: .31rem;
}
.verticalPage .swiper-slide .detail p.txt{
    color: #222;
    font-size: .12rem;
    line-height: .2rem;
    width: 80%;
    margin:0 auto;
    text-indent: 24px;
    padding-bottom: .11rem;
}
.verticalPage .swiper-slide p.botm{
    position: absolute;
    left: 10%;
    bottom: 1.1rem;
    color: #222;
    font-size: .12rem;
    line-height: .2rem;
    width: 80%;
    text-indent: 24px;
    padding-bottom: .15rem;
}
.verticalPage .swiper-slide .chinaImg{
    width: 80%;
    margin: 0 auto;
    display: block;
}

.detail_table{
    width: 80%;
    margin:0 auto .2rem;
}
.detail_table table{
    width: 100%;
    border: none;
    text-align: center;
}
.detail_table table thead{
    background: #c5c1b2;
    color: #fff;
}
.detail_table table thead th{
    height: .3rem;
    font-size: .11rem;
    font-weight: normal;
}
.detail_table table tbody tr{
    height: .4rem;
    font-size: .10rem;
    font-weight: normal;
    color: #222;
}
.downloadBtn{
    width: 1.65rem;
    height: .4rem;
    line-height: .4rem;
    font-size: .13rem;
    color: #fff;
    text-align: center;
    position: absolute;
    left: 50%;
    margin-left: -.825rem;
    bottom: .65rem;
    background: #05a1ff;
}

