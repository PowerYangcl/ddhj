               appearance:button;
               -moz-appearance:button; /* Firefox */
               -webkit-appearance:button; /* Safari 鍜� Chrome */
}
html {
    background: #f8f8f8;
}

table {
    border-collapse: collapse;
}
.cel-26 {
    width: 26%;
}
.cel-37 {
    width: 37%;
}
.verticalPage{position: fixed; width: 100%;}
.verticalPage ul{
     width: 100%;
    position: absolute;
    left: 0;
    top: 0;
    webkit-transition: 1s all ease;
}
.verticalPage .swiper-slide{
    position: relative;
    background: url(../../images/report/bg_back02.png) no-repeat;
    background-size: 100% 100%;
}
.verticalPage .swiper-slide:first-child{
    background: url(../../images/report/bg_back.png) no-repeat;
    background-size: 100% 100%;
}
.verticalPage .swiper-slide .bg_logo01{
    width: 100%;
    margin-top: .9rem;
}
.verticalPage .swiper-slide h1{
    margin-top: .3rem;
    font-size: .25rem;
    color: #222;
    text-align: center;
}
.verticalPage .swiper-slide h1 span{
    position: relative;
    line-height: .25rem;
}
.verticalPage .swiper-slide h1 span:before{
    content: "";
    position: absolute;
    height: 1px;
    background: #222;
    width: .45rem;
    top: .18rem;
    left: -.6rem;
}
.verticalPage .swiper-slide h1 span:after{
    content: "";
    position: absolute;
    height: 1px;
    background: #222;
    width: .45rem;
    top: .18rem;
    right: -.6rem;
}
.verticalPage .swiper-slide .time{
    font-size: .12rem;
    color: #222;
    text-align: center;
    margin-top: .12rem;
}
.verticalPage .swiper-slide .position{
    font-size: .18rem;
    color: #0076be;
    text-align: center;
    margin-top: .7rem;
}
.verticalPage .swiper-slide .copyright{
    font-size: .14rem;
    color: #222;
    position: absolute;
    text-align: center;
    width: 100%;
    left: 0;
    bottom: .7rem;
}
